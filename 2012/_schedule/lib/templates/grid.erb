<article id="subEvent">
  <h2><%= t(:sub_event) %></h2>
  <%- %w(4 3 2 1).each do |sub_event_id| -%>
    <%- sub_event = sub_event(sub_event_id) -%>
    <%- next if sub_event.title.nil? %>
    <h3 id="<%= sub_event_html_id(sub_event_id) %>"><%= sub_event.title %></h3>
    <%= md(sub_event.abstract) %>
    <ul class="listMarkRed">
      <%- sub_event.others.each do |other| -%>
        <li><%= md(other) %></li>
      <%- end -%>
    </ul>
  <%- end %>
  <p class="backToTop"><a href="#">Back to Top</a></p>
